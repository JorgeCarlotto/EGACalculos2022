<%- include('layouts/header') %> 
<body>
   
    <h2>Carga de Datos</h2>
    <div class="container">
        <form class="row g-1" method="post" action="/">
    
            <div class="form-group">
                <label for="my-input">Hora</label>
                <input id="my-input" class="form-control" type="number" name="hora">
            </div>
    
            <div class="form-group">
                <label for="my-input">Nombre del Barco</label>
                <input id="my-input" class="form-control" type="text" name="nombreBarco">
            </div>

            <div class="form-group">
                <label for="my-input">Velocidad Barco</label>
                <input id="my-input" class="form-control" type="number" name="velocidad">
            </div>

            <div class="form-group">
                <label for="my-input">Rumbo Barco</label>
                <input id="my-input" class="form-control" type="number" name="rumbo">
            </div>

            <div class="form-group">
                <label for="my-input">Cantidad de horas a Navegar</label>
                <input id="my-input" class="form-control" type="number" name="horas">
            </div>
    
    
            <h2>Latitud</h2>
    
            <div class="form-group">
                <label for="my-input">Grados</label>
                <input id="my-input" class="form-control" type="number" name="latGrados">
            </div>
    
            <div class="form-group">
                <label for="my-input">Minutos</label>
                <input id="my-input" class="form-control" type="number" name="latMinutos">
            </div>
    
            <div class="form-group">
                <label for="my-input">Segundos</label>
                <input id="my-input" class="form-control" type="number" name="latSegundos">
            </div>
    
            <h2>Longitud</h2>
    
            <div class="form-group">
                <label for="my-input">Grados</label>
                <input id="my-input" class="form-control" type="number" name="longGrados">
            </div>
    
            <div class="form-group">
                <label for="my-input">Minutos</label>
                <input id="my-input" class="form-control" type="number" name="longMinutos">
            </div>
    
            <div class="form-group">
                <label for="my-input">Segundos</label>
                <input id="my-input" class="form-control" type="number" name="longSegundos">
            </div>
    
            <h4>Tipo de buque</h4>
    
            <div class="form-group">
                <label for="my-select">Tipo de Buques</label>
                <select id="my-select" class="custom-select" name="tipoDeBuque">
                    <option>PESQUERO</option>
                    <option>CARGA</option>
                    <option>SIN DATO</option>
                </select>
            </div>
    
            <h4>Actividad</h4>
    
            <div class="form-group">
                <label for="my-select">Actividad</label>
                <select id="my-select" class="custom-select" name="actividad">
                    <option>PESCANDO</option>
                    <option>NAVEGANDO</option>
                    <option>FONDEADO</option>
                    <option>SIN DATO</option>
                    <option>Otro</option>
                </select>
            </div>
    
    
            <h4>Metodo de detecci√≥n</h4>
    
            <div class="form-group">
                <label for="my-select">Tipo de Buques</label>
                <select id="my-select" class="custom-select" name="detectado">
                    <option>VISUAL</option>
                    <option>RADAR</option>
                    <option>AIS</option>
                    <option>Otro</option>
                </select>
            </div>
    
    
            <button type="submit" class="btn btn-primary">Cargar Barco</button>
          </form>
        </div>



</body>

<%- include('layouts/footer') %> 
